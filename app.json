{
  "expo": {
    "name": "MFYMobile",
    "slug": "MFYMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "scheme": "mfymobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Joylashuvingizni ish vaqtida kuzatish uchun ruxsat kerak.",
        "NSLocationAlwaysUsageDescription": "Joylashuvingizni fon rejimida ham kuzatish uchun ruxsat kerak.",
        "NSLocationWhenInUseUsageDescription": "Joylashuvingizni aniqlash uchun ruxsat kerak."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0f1b2d",
        "foregroundImage": "./assets/images/app_icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.azamatakoooo.MFYMobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/app_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash_screen.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f1b2d",
          "dark": {
            "backgroundColor": "#0f1b2d"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Joylashuvingizni ish vaqtida kuzatish uchun ruxsat kerak.",
          "locationAlwaysPermission": "Joylashuvingizni fon rejimida ham kuzatish uchun ruxsat kerak.",
          "locationWhenInUsePermission": "Joylashuvingizni aniqlash uchun ruxsat kerak.",
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Ishga kelganingizni tasdiqlash uchun rasm olish kerak."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/app_icon.png",
          "color": "#0ea5e9",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ea5fcb38-d9cd-4bdb-99d5-db089af456dd"
      }
    }
  }
}
